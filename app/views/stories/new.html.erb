<div class="container">
  <%= form_with(model: @story, local: true) do |form| %>
    <div class="form-group">
      <%= form.file_field :images, class:"form-control", type:"file", id:"formFile", multiple: true, direct_upload: true, accept: 'image/*'%>
    </div>
    <div class="form-group">
      <%= form.number_field :user_id, id: :story_user_id, class:"form-control", value: current_user.id, type: :hidden %>
    </div>
    <div class="btn btn-defaults">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  <% end %>
</div>
